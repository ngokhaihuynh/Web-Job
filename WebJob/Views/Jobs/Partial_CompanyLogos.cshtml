@model IEnumerable<WebJob.Models.EF.CompanyImage>

<div class="row">
    @foreach (var image in Model)
    {
        <div class="col-md-3 col-sm-6 col-12 top-employer-wrap">
            <div class="top-employer-item">
                <a href="#">
                    <div class="emp-img-thumb">
                        <img src="@image.ImageURL" alt="Logo của @image.Company.CompanyName">
                    </div>
                    <h3 class="company">@image.Company.CompanyName</h3>
                </a>
            </div>
        </div>
    }
    
</div>
